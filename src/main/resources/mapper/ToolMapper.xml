<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.uncleray.web.dao.mapper.ToolMapper">

    <select id="selectByToolSeq" resultType="java.util.HashMap">
        SELECT toolName,toolDesc FROM t_tool WHERE toolseq=#{toolSeq}
    </select>
</mapper>